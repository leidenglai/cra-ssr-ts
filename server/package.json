{
  "name": "cra-ssr-ts-server",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.serv.conf.js",
    "dev-start": "NODE_ENV=production PORT=8000 node ./build/server.js",
    "start": "yarn install --production && pm2 start config/pm2.json",
    "restart": "yarn install --production && pm2 restart config/pm2.json",
    "stop": "pm2 stop config/pm2.json"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "stylelintIntegration": true,
    "eslintIntegration": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "body-parser": "^1.19.0",
    "connected-react-router": "^4.5.0",
    "ignore-styles": "^5.0.1",
    "immer": "^3.1.3",
    "intl": "1.2.5",
    "js-cookie": "^2.2.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lru-cache": "^5.1.1",
