    "node-fetch": "2.6.7",
    "prop-types": "15.7.2",
    "query-string": "^6.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-frontload": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.1.1",
